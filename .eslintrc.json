{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-use-before-define": ["off"],
        "max-lines-per-function": ["off"],
        "consistent-return": ["off"],
        "import/order":["off"],
        "no-console":["off"],
        "arrow-body-style":["off"],
        "camelcase":["off"]
    }
}
